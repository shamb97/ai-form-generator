    <!-- FORM COMPLETION MODAL - NEW! -->
    <div class="modal-overlay" id="formCompletionModal">
        <div class="form-modal">
            <div class="modal-header">
                <h3 id="formModalTitle">üìã Complete Form</h3>
                <button class="modal-close" onclick="closeFormModal()">√ó</button>
            </div>
            <div class="modal-body">
                <div id="formModalFields">
                    <!-- Form fields will be rendered here -->
                </div>
            </div>
            <div class="modal-footer">
                <button class="modal-button cancel" onclick="closeFormModal()">Cancel</button>
                <button class="modal-button confirm" onclick="submitFormCompletion()" id="submitFormBtn">
                    Save Form
                </button>
            </div>
        </div>
    </div>

    <script>
        const API_BASE = 'http://localhost:8000';
        let allSubjects = [];
        let filteredSubjects = [];
        let currentSubjectDetail = null;
        
        // Form completion variables
        let currentFormData = null;
        let currentFormValues = {};
        let skipLogicRules = [];
        let currentCompletedBy = null;
        
        // Load dashboard on page load
        window.addEventListener('load', () => {
            loadDashboard();
        });
        
        // ========================================
        // LOAD DASHBOARD DATA
        // ========================================
        async function loadDashboard() {
            try {
                const response = await fetch(`${API_BASE}/api/v1/test/investigator-subjects`);
                if (!response.ok) throw new Error('Failed to fetch');
                
                const data = await response.json();
                
                // Update header
                document.getElementById('investigatorName').textContent = 
                    `${data.investigator_name} ‚Ä¢ ${data.site_name}`;
                
                // Store subjects
                allSubjects = data.subjects || [];
                filteredSubjects = [...allSubjects];
                
                // Render
                renderStats();
                renderSubjects();
                
            } catch (error) {
                console.error('Error loading dashboard:', error);
                alert('‚ö†Ô∏è Failed to load dashboard. Please ensure backend is running on port 8000.');
            }
        }
        
        // ========================================
        // RENDER STATS
        // ========================================
        function renderStats() {
            const totalSubjects = allSubjects.length;
            const totalFormsDue = allSubjects.reduce((sum, s) => sum + s.investigator_forms_due, 0);
            const subjectsNeedAttention = allSubjects.filter(s => s.investigator_forms_due > 0).length;
            
            document.getElementById('statsGrid').innerHTML = `
                <div class="stat-card">
                    <div class="stat-card-header">
                        <span class="stat-icon">üë•</span>
                    </div>
                    <div class="stat-number">${totalSubjects}</div>
                    <div class="stat-label">Total Subjects</div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-card-header">
                        <span class="stat-icon">üìã</span>
                    </div>
                    <div class="stat-number alert">${totalFormsDue}</div>
                    <div class="stat-label">Forms Due Today</div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-card-header">
                        <span class="stat-icon">‚ö†Ô∏è</span>
                    </div>
                    <div class="stat-number">${subjectsNeedAttention}</div>
                    <div class="stat-label">Need Attention</div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-card-header">
                        <span class="stat-icon">‚úÖ</span>
                    </div>
                    <div class="stat-number success">${totalSubjects - subjectsNeedAttention}</div>
                    <div class="stat-label">All Complete</div>
                </div>
            `;
        }
        
        // ========================================
        // RENDER SUBJECTS
        // ========================================
        function renderSubjects() {
            const container = document.getElementById('subjectsContainer');
            const emptyState = document.getElementById('emptyState');
            
            if (filteredSubjects.length === 0) {
                container.innerHTML = '';
                emptyState.style.display = 'block';
                return;
            }
            
            emptyState.style.display = 'none';
            
            container.innerHTML = filteredSubjects.map(subject => {
                const phaseLower = subject.phase.toLowerCase().replace(/[-\s]/g, '');
                const statusIcon = subject.investigator_forms_due > 0 ? 'üî¥' : '‚úÖ';
                
                return `
                    <div class="subject-card" onclick="loadSubjectDetail('${subject.subject_id}')">
                        <div class="subject-main">
                            <div class="subject-name-row">
                                <span class="subject-name">${subject.display_name}</span>
                                <span class="phase-badge ${phaseLower}">${subject.phase}</span>
                            </div>
                            <div class="subject-meta-grid">
                                <div class="meta-item">
                                    <strong>Study Day:</strong> ${subject.study_day}
                                </div>
                                <div class="meta-item">
                                    <strong>Last Visit:</strong> ${subject.last_visit}
                                </div>
                                <div class="meta-item">
                                    <strong>Code:</strong> ${subject.subject_id}
                                </div>
                            </div>
                        </div>
                        <div class="subject-actions">
                            <div class="status-indicator">${statusIcon}</div>
                            ${subject.investigator_forms_due > 0 ? 
                                `<div class="forms-due-badge">
                                    üî¥ ${subject.investigator_forms_due} Due
                                </div>` :
                                `<div class="forms-complete-badge">‚úÖ Complete</div>`
                            }
                        </div>
                    </div>
                `;
            }).join('');
        }
        
        // ========================================
        // FILTER SUBJECTS
        // ========================================
        function filterSubjects() {
            const searchTerm = document.getElementById('searchInput').value.toLowerCase();
            const phaseFilter = document.getElementById('phaseFilter').value;
            const statusFilter = document.getElementById('statusFilter').value;
            
            filteredSubjects = allSubjects.filter(subject => {
                const matchesSearch = 
                    subject.display_name.toLowerCase().includes(searchTerm) ||
                    subject.subject_id.toLowerCase().includes(searchTerm);
                
                const matchesPhase = 
                    phaseFilter === 'all' || 
                    subject.phase.toLowerCase() === phaseFilter;
                
                let matchesStatus = true;
                if (statusFilter === 'forms_due') {
                    matchesStatus = subject.investigator_forms_due > 0;
                } else if (statusFilter === 'complete') {
                    matchesStatus = subject.investigator_forms_due === 0;
                }
                
                return matchesSearch && matchesPhase && matchesStatus;
            });
            
            sortSubjects();
        }
        
        // ========================================
        // SORT SUBJECTS
        // ========================================
        function sortSubjects() {
            const sortBy = document.getElementById('sortBy').value;
            
            filteredSubjects.sort((a, b) => {
                switch (sortBy) {
                    case 'forms_due':
                        return b.investigator_forms_due - a.investigator_forms_due;
                    case 'name':
                        return a.display_name.localeCompare(b.display_name);
                    case 'study_day':
                        return a.study_day - b.study_day;
                    case 'last_visit':
                        return new Date(b.last_visit) - new Date(a.last_visit);
                    default:
                        return 0;
                }
            });
            
            renderSubjects();
        }
        
        // ========================================
        // LOAD SUBJECT DETAIL
        // ========================================
        async function loadSubjectDetail(subjectId) {
            try {
                const response = await fetch(`${API_BASE}/api/v1/test/subject-detail/${subjectId}`);
                if (!response.ok) throw new Error('Failed to fetch');
                
                const data = await response.json();
                currentSubjectDetail = data;
                
                renderSubjectDetail(data);
                showDetailView();
                
            } catch (error) {
                console.error('Error loading subject detail:', error);
                alert('Failed to load subject details');
            }
        }
        
        // ========================================
        // RENDER SUBJECT DETAIL
        // ========================================
        function renderSubjectDetail(data) {
            const phaseLower = data.phase.toLowerCase().replace(/[-\s]/g, '');
            
            // Header
            document.getElementById('detailHeader').innerHTML = `
                <div class="detail-header-top">
                    <div class="detail-header-left">
                        <h2>üë§ ${data.display_name}</h2>
                        <span class="phase-badge ${phaseLower}" style="font-size: 14px; padding: 6px 14px;">
                            ${data.phase}
                        </span>
                    </div>
                    <button class="phase-transition-btn" onclick="openPhaseTransitionModal()">
                        üîÑ Change Phase
                    </button>
                </div>
                
                <div class="detail-meta-grid">
                    <div class="detail-meta-item">
                        <div class="detail-meta-label">Subject Code</div>
                        <div class="detail-meta-value">${data.subject_code}</div>
                    </div>
                    <div class="detail-meta-item">
                        <div class="detail-meta-label">Study Day</div>
                        <div class="detail-meta-value">${data.study_day}</div>
                    </div>
                    <div class="detail-meta-item">
                        <div class="detail-meta-label">Current Phase</div>
                        <div class="detail-meta-value">${data.phase}</div>
                    </div>
                    <div class="detail-meta-item">
                        <div class="detail-meta-label">Last Visit</div>
                        <div class="detail-meta-value">${data.last_visit || 'N/A'}</div>
                    </div>
                </div>
            `;
            
            // Progress
            const totalForms = data.investigator_forms.length + data.subject_forms.length;
            const completedForms = data.investigator_forms.filter(f => f.status === 'complete').length +
                                  data.subject_forms.filter(f => f.status === 'complete').length;
            const progressPct = totalForms > 0 ? Math.round((completedForms / totalForms) * 100) : 0;
            
            document.getElementById('progressSection').innerHTML = `
                <h3>üìä Overall Progress</h3>
                <div class="progress-bar-container">
                    <div class="progress-bar-header">
                        <div class="progress-bar-label">Total Forms</div>
                        <div class="progress-bar-value">${completedForms} / ${totalForms} (${progressPct}%)</div>
                    </div>
                    <div class="progress-bar">
                        <div class="progress-bar-fill" style="width: ${progressPct}%"></div>
                    </div>
                </div>
            `;
            
            // Investigator Forms
            const invFormsHtml = data.investigator_forms.map(form => {
                if (form.status === 'complete') {
                    return `
                        <div class="form-item complete">
                            <div class="form-item-left">
                                <div class="form-item-title">
                                    ‚úÖ ${form.form_name}
                                </div>
                                <div class="form-item-meta">
                                    Completed: ${form.completed_date}
                                </div>
                            </div>
                            <button class="action-button secondary" onclick="viewFormData('${form.form_id}')">
                                View Data
                            </button>
                        </div>
                    `;
                } else {
                    return `
                        <div class="form-item pending">
                            <div class="form-item-left">
                                <div class="form-item-title">
                                    üìã ${form.form_name}
                                </div>
                                <div class="form-item-meta ${form.status === 'overdue' ? 'overdue' : ''}">
                                    ${form.status === 'overdue' ? '‚ö†Ô∏è Overdue: ' : 'Due: '} ${form.due_date}
                                </div>
                            </div>
                            <button class="action-button primary" onclick="completeForm('${form.form_id}', 'investigator')">
                                Complete Form
                            </button>
                        </div>
                    `;
                }
            }).join('');
            
            document.getElementById('investigatorForms').innerHTML = invFormsHtml || 
                '<p style="color: #999; text-align: center; padding: 20px;">No investigator forms at this time</p>';
            
            document.getElementById('invFormsCount').textContent = data.investigator_forms.length;
            
            // Subject Forms
            const subjFormsHtml = data.subject_forms.map(form => {
                const statusIcon = form.status === 'complete' ? '‚úÖ' : '‚è≥';
                return `
                    <div class="form-item ${form.status === 'complete' ? 'complete' : ''}">
                        <div class="form-item-left">
                            <div class="form-item-title">
                                ${statusIcon} ${form.form_name}
                            </div>
                            <div class="form-item-meta">
                                ${form.status === 'complete' ? 
                                    `Completed: ${form.completed_date}` : 
                                    `Due: ${form.due_date}`
                                }
                            </div>
                        </div>
                        ${form.status === 'complete' ? 
                            `<button class="action-button secondary" onclick="viewFormData('${form.form_id}')">
                                View Data
                            </button>` : 
                            ''
                        }
                    </div>
                `;
            }).join('');
            
            document.getElementById('subjectForms').innerHTML = subjFormsHtml || 
                '<p style="color: #999; text-align: center; padding: 20px;">No subject forms available</p>';
            
            document.getElementById('subjFormsCount').textContent = data.subject_forms.length;
        }
        
        // ========================================
        // PHASE TRANSITION MODAL
        // ========================================
        async function openPhaseTransitionModal() {
            if (!currentSubjectDetail) return;
            
            const currentPhase = currentSubjectDetail.phase.toLowerCase();
            
            document.getElementById('currentPhaseDisplay').textContent = currentSubjectDetail.phase;
            document.getElementById('transitionSubjectName').textContent = currentSubjectDetail.display_name;
            document.getElementById('phaseWarning').style.display = 'none';
            
            try {
                const response = await fetch(`${API_BASE}/api/v1/phases/rules/1/${currentPhase}`);
                const data = await response.json();
                
                if (data.success && data.allowed_transitions.length > 0) {
                    const dropdown = document.getElementById('newPhase');
                    dropdown.innerHTML = '<option value="">-- Choose Phase --</option>';
                    
                    data.allowed_transitions.forEach(phase => {
                        const option = document.createElement('option');
                        option.value = phase;
                        option.textContent = phase.charAt(0).toUpperCase() + phase.slice(1);
                        dropdown.appendChild(option);
                    });
                } else {
                    alert('No valid phase transitions available from current phase');
                    return;
                }
                
            } catch (error) {
                console.error('Error loading phase transitions:', error);
                alert('Failed to load phase transition options');
                return;
            }
            
            document.getElementById('phaseTransitionModal').classList.add('active');
        }

        function closePhaseModal() {
            document.getElementById('phaseTransitionModal').classList.remove('active');
        }
        
        async function confirmPhaseTransition() {
            const newPhase = document.getElementById('newPhase').value;
            
            if (!newPhase) {
                alert('Please select a new phase');
                return;
            }
            
            if (!currentSubjectDetail) return;
            
            const currentPhase = currentSubjectDetail.phase.toLowerCase();
            if (currentPhase === newPhase) {
                alert('Subject is already in this phase');
                return;
            }
            
            try {
                const response = await fetch(`${API_BASE}/api/v1/phases/transition`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        study_id: 1,
                        subject_id: currentSubjectDetail.subject_id,
                        from_phase: currentPhase,
                        to_phase: newPhase,
                        transition_day: currentSubjectDetail.study_day,
                        triggered_by: 'investigator',
                        investigator_id: 'INV001'
                    })
                });
                
                const result = await response.json();
                
                if (result.success) {
                    alert(`‚úÖ Phase Transition Successful!\n\nSubject: ${currentSubjectDetail.display_name}\nFrom: ${currentPhase}\nTo: ${newPhase}\nTransition ID: ${result.transition_id}\n\nAudit trail created!`);
                    currentSubjectDetail.phase = newPhase.charAt(0).toUpperCase() + newPhase.slice(1);
                    renderSubjectDetail(currentSubjectDetail);
                    closePhaseModal();
                    setTimeout(() => loadDashboard(), 500);
                } else {
                    alert(`‚ùå Phase Transition Failed!\n\n${result.error}`);
                }
                
            } catch (error) {
                console.error('Error executing phase transition:', error);
                alert('Failed to execute phase transition. Please try again.');
            }
        }
        
        // ========================================
        // FORM COMPLETION - COMPLETE IMPLEMENTATION
        // ========================================
        async function completeForm(formId, completedBy) {
            console.log(`üìã Opening form: ${formId} (completed by: ${completedBy})`);
            
            currentCompletedBy = completedBy;
            
            // For now, load a demo form
            // In production, this would fetch from backend
            await loadFormSchema(formId);
        }

        async function loadFormSchema(formId) {
            // Demo form with skip logic
            const demoForm = {
                form_id: formId,
                title: "Medical Assessment Form",
                description: "Complete medical assessment with skip logic",
                fields: [
                    {
                        field_id: 'has_symptoms',
                        label: 'Are you experiencing any symptoms?',
                        type: 'radio',
                        required: true,
                        options: [
                            { value: 'yes', label: 'Yes' },
                            { value: 'no', label: 'No' }
                        ]
                    },
                    {
                        field_id: 'symptom_details',
                        label: 'Please describe your symptoms',
                        type: 'textarea',
                        required: true,
                        placeholder: 'Describe symptoms in detail...',
                        skip_logic: {
                            show_when: 'has_symptoms',
                            equals: 'yes'
                        }
                    },
                    {
                        field_id: 'pain_level',
                        label: 'Pain level (0-10)',
                        type: 'number',
                        required: true,
                        min: 0,
                        max: 10,
                        skip_logic: {
                            show_when: 'has_symptoms',
                            equals: 'yes'
                        }
                    },
                    {
                        field_id: 'vital_signs',
                        label: 'Record vital signs',
                        type: 'text',
                        required: true,
                        placeholder: 'BP, HR, Temp, etc.'
                    },
                    {
                        field_id: 'notes',
                        label: 'Additional notes',
                        type: 'textarea',
                        required: false,
                        placeholder: 'Any additional observations...'
                    }
                ]
            };

            currentFormData = demoForm;
            currentFormValues = {};
            
            // Extract skip logic rules
            skipLogicRules = demoForm.fields
                .filter(f => f.skip_logic)
                .map(f => ({
                    field_id: f.field_id,
                    show_when: f.skip_logic.show_when,
                    equals: f.skip_logic.equals
                }));

            renderFormInModal();
            document.getElementById('formCompletionModal').classList.add('active');
        }

        function renderFormInModal() {
            if (!currentFormData) return;

            document.getElementById('formModalTitle').textContent = `üìã ${currentFormData.title}`;

            const fieldsHtml = currentFormData.fields.map(field => {
                const isConditional = field.skip_logic ? true : false;
                const shouldHide = isConditional;

                return `
                    <div class="form-field-group ${shouldHide ? 'hidden' : ''}" id="field_${field.field_id}">
                        <label class="field-label">
                            ${field.label}
                            ${field.required ? '<span class="required">*</span>' : ''}
                            ${isConditional ? '<span class="skip-logic-indicator">Conditional</span>' : ''}
                        </label>
                        ${renderFieldInput(field)}
                        ${field.help ? `<div class="field-help">${field.help}</div>` : ''}
                        <div class="field-error" id="error_${field.field_id}"></div>
                    </div>
                `;
            }).join('');

            document.getElementById('formModalFields').innerHTML = fieldsHtml;
            attachFormEventListeners();
        }

        function renderFieldInput(field) {
            switch(field.type) {
                case 'text':
                case 'email':
                case 'tel':
                case 'number':
                    return `<input 
                        type="${field.type}" 
                        id="${field.field_id}" 
                        class="field-input"
                        placeholder="${field.placeholder || ''}"
                        ${field.required ? 'required' : ''}
                        ${field.min !== undefined ? `min="${field.min}"` : ''}
                        ${field.max !== undefined ? `max="${field.max}"` : ''}
                    />`;
                
                case 'textarea':
                    return `<textarea 
                        id="${field.field_id}" 
                        class="field-input"
                        rows="4"
                        placeholder="${field.placeholder || ''}"
                        ${field.required ? 'required' : ''}
                    ></textarea>`;
                
                case 'radio':
                    return `<div class="radio-group">
                        ${field.options.map(opt => `
                            <label class="radio-option">
                                <input 
                                    type="radio" 
                                    name="${field.field_id}" 
                                    value="${opt.value}"
                                    ${field.required ? 'required' : ''}
                                />
                                <span>${opt.label}</span>
                            </label>
                        `).join('')}
                    </div>`;
                
                case 'checkbox':
                    return `<div class="checkbox-group">
                        ${field.options.map(opt => `
                            <label class="checkbox-option">
                                <input 
                                    type="checkbox" 
                                    name="${field.field_id}" 
                                    value="${opt.value}"
                                />
                                <span>${opt.label}</span>
                            </label>
            if (field.type === 'radio') {
                const checked = document.querySelector(`input[name="${fieldId}"]:checked`);
                value = checked ? checked.value : null;
            } else if (field.type === 'checkbox') {
                const checked = Array.from(document.querySelectorAll(`input[name="${fieldId}"]:checked`));
                value = checked.map(c => c.value);
            } else {
                const element = document.getElementById(fieldId);
                value = element ? element.value : null;
            }

            currentFormValues[fieldId] = value;
            evaluateFormSkipLogic();
        }

        function evaluateFormSkipLogic() {
            skipLogicRules.forEach(rule => {
                const triggerValue = currentFormValues[rule.show_when];
                const shouldShow = triggerValue === rule.equals;

                const fieldElement = document.getElementById(`field_${rule.field_id}`);
                if (fieldElement) {
                    if (shouldShow && fieldElement.classList.contains('hidden')) {
                        fieldElement.classList.remove('hidden');
                        fieldElement.classList.add('just-appeared');
                        setTimeout(() => {
                            fieldElement.classList.remove('just-appeared');
                        }, 300);
                    } else if (!shouldShow && !fieldElement.classList.contains('hidden')) {
                        fieldElement.classList.add('hidden');
                        const inputElement = document.getElementById(rule.field_id);
                        if (inputElement) {
                            inputElement.value = '';
                            delete currentFormValues[rule.field_id];
                        }
                    }
                }
            });
        }

        async function submitFormCompletion() {
            // Clear previous errors
            document.querySelectorAll('.field-error').forEach(el => {
                el.classList.remove('visible');
            });
            document.querySelectorAll('.field-input').forEach(el => {
                el.classList.remove('error');
            });
        
            let hasErrors = false;
            const errors = [];
        
            // Validate only VISIBLE required fields
            currentFormData.fields.forEach(field => {
                const fieldContainer = document.getElementById(`field_${field.field_id}`);
                const isVisible = fieldContainer && !fieldContainer.classList.contains('hidden');
            
                if (isVisible && field.required) {
                    const value = currentFormValues[field.field_id];
                    const isEmpty = !value || (Array.isArray(value) && value.length === 0) || 
                                   (typeof value === 'string' && value.trim() === '');
                
                    if (isEmpty) {
                        hasErrors = true;
                        errors.push({
                            field_id: field.field_id,
                            label: field.label,
                            message: `${field.label} is required`
                        });
                    
                        const errorEl = document.getElementById(`error_${field.field_id}`);
                        if (errorEl) {
                            errorEl.textContent = 'This field is required';
                            errorEl.classList.add('visible');
                        }
                    
                        const inputEl = document.getElementById(field.field_id);
                        if (inputEl) {
                            inputEl.classList.add('error');
                        }
                    }
                }
            });
        
            if (hasErrors) {
                alert(`‚ùå Please fill in all required fields!\n\nMissing:\n${errors.map(e => `- ${e.label}`).join('\n')}`);
                const firstError = document.querySelector('.field-error.visible');
                if (firstError) {
                    firstError.parentElement.scrollIntoView({ behavior: 'smooth', block: 'center' });
                }
                return;
            }
        
            // Disable submit button
            const submitBtn = document.getElementById('submitFormBtn');
            submitBtn.disabled = true;
            submitBtn.textContent = 'Saving...';
        
            try {
                // Call backend API
                const response = await fetch(`${API_BASE}/api/v1/forms/submit`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        study_id: 1,
                        subject_id: currentSubjectDetail.subject_id,
                        form_id: currentFormData.form_id,
                        form_data: currentFormValues,
                        completed_by: currentCompletedBy,
                        investigator_id: currentCompletedBy === 'investigator' ? 'INV001' : null
                    })
                });
            
                const result = await response.json();
            
                if (result.success) {
                    console.log('‚úÖ Form saved to database!', result);
                    
                    alert(`‚úÖ Form Submitted Successfully!\n\n` +
                          `Form: ${currentFormData.title}\n` +
                          `Completion ID: ${result.completion_id}\n` +
                          `Completed by: ${currentCompletedBy}\n\n` +
                          `Data saved to database!`);
                
                    closeFormModal();
                
                    // Reload subject detail to show updated status
                    if (currentSubjectDetail) {
                        await loadSubjectDetail(currentSubjectDetail.subject_id);
                    }
                } else {
                    throw new Error(result.error || 'Failed to save form');
                }
            
            } catch (error) {
                console.error('‚ùå Form submission error:', error);
                alert(`‚ùå Failed to save form!\n\nError: ${error.message}\n\nPlease try again.`);
                
                // Re-enable submit button
                submitBtn.disabled = false;
                submitBtn.textContent = 'Save Form';
            }
        }

            if (hasErrors) {
                alert(`‚ùå Please fill in all required fields!\n\nMissing:\n${errors.map(e => `- ${e.label}`).join('\n')}`);
                const firstError = document.querySelector('.field-error.visible');
                if (firstError) {
                    firstError.parentElement.scrollIntoView({ behavior: 'smooth', block: 'center' });
                }
                return;
            }

            // Success! Show submission data
            const submissionData = {
                form_id: currentFormData.form_id,
                form_title: currentFormData.title,
                completed_by: currentCompletedBy,
                subject_id: currentSubjectDetail ? currentSubjectDetail.subject_id : 'unknown',
                submitted_at: new Date().toISOString(),
                values: currentFormValues
            };

            alert('‚úÖ Form submitted successfully!\n\nCheck console for full data.');
            console.log('üìã FORM SUBMISSION DATA:', submissionData);

            closeFormModal();

            // Reload subject detail to update progress
            if (currentSubjectDetail) {
                await loadSubjectDetail(currentSubjectDetail.subject_id);
            }
        }

        function closeFormModal() {
            document.getElementById('formCompletionModal').classList.remove('active');
            currentFormData = null;
            currentFormValues = {};
            skipLogicRules = [];
        }
        
        function viewFormData(formId) {
            alert(`üëÅÔ∏è Viewing Form Data: ${formId}\n\nWould show completed form data in read-only view.`);
        }
        
        // ========================================
        // VIEW NAVIGATION
        // ========================================
        function showDetailView() {
            document.getElementById('listView').style.display = 'none';
            document.getElementById('detailView').classList.add('active');
            window.scrollTo(0, 0);
        }
        
        function showListView() {
            document.getElementById('detailView').classList.remove('active');
            document.getElementById('listView').style.display = 'block';
            window.scrollTo(0, 0);
        }
        
        // Close modals on outside click
        document.getElementById('phaseTransitionModal').addEventListener('click', (e) => {
            if (e.target.id === 'phaseTransitionModal') {
                closePhaseModal();
            }
        });

        document.getElementById('formCompletionModal').addEventListener('click', (e) => {
            if (e.target.id === 'formCompletionModal') {
                closeFormModal();
            }
        });
    </script>
</body>
</html>